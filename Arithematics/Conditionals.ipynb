    "# Dont divide anything by 0\n",
    "from IPython.display import display, Math\n",
    "for i in range(0, 4):\n",
    "    for j in range(0, 5):\n",
    "        if(i > 0 and j > 0 ):\n",
    "            display(Math( '%g^-%g = %g ' %(i, j, i**-j) ))"
